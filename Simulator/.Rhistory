plot( GIs[ tind,1], x = tind, type='l', ylab="Global inhibitor", xlab ="GI_speed", main="Time 0-1000",  ylim = c(0, max(GIs)), col=alpha("black", 0.3))
for (i in 2:100)
{
lines( GIs[tind,i], x = tind, col = alpha(i, 0.3))
}
